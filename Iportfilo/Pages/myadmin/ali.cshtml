        <div class="row">
            <div class="col-md-6">
                <h1>
                    Skills
                </h1>
            </div>
        </div>
        <div class="row">
            <div class="col-md-6 justify-content-center">
                <label class="form-label"> Skill Name </label>
                <input class="form-control" asp-for="skills.Name" />
            </div>
        </div>
        <div class="row">
            <div class="col-md-6 justify-content-center">
                <label class="form-label"> Percentege </label>
                <input class="form-control" asp-for="skills.Percentage" />
