@page
@model Iportfilo.Pages.myadmin.editserviceModel
@{
}
<div class="container">
    <form method="post">
        <div class="row">
            <div class="col-md-6">
                <h1>
                    Update Services
                </h1>
            </div>
        </div>
        <div class="row">
            <div class="col-md-6 justify-content-center">
                <label class="form-label"> Icon Name </label>
                <input class="form-control" asp-for="services.IconeName" />
                <input class="form-control" Hidden asp-for="services.Id" />
            </div>
        </div>
        <div class="row">
            <div class="col-md-6 justify-content-center">
                <label class="form-label"> Title </label>
                <input class="form-control" asp-for="services.Title" />
            </div>
        </div>
        <div class="row">
            <input type="submit" value="Update Services Info" class="btn btn-primary" />
        </div>
    </form>
</div>