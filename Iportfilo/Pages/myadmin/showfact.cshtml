@page
@model Iportfilo.Pages.myadmin.showfactModel
@{
}
<div class="container">
    <form method="post">
        <div class="row">
            <div class="col-md-6">
                <h1>
                    Update Facts
                </h1>
            </div>
        </div>
        <div class="row">
            <div class="col-md-6 justify-content-center">
                <label class="form-label"> Degree </label>
                <input class="form-control" asp-for="fact.client" />
                <input class="form-control" Hidden asp-for="fact.Id" />
            </div>
        </div>
        <div class="row">
            <div class="col-md-6 justify-content-center">
                <label class="form-label"> startyear </label>
                <input class="form-control" asp-for="fact.Hours" />
            </div>
        </div>
        <div class="row">
            <div class="col-md-6 justify-content-center">
                <label class="form-label"> endyear </label>
                <input class="form-control" asp-for="fact.Hardwork" />
            </div>
        </div>
        <div class="row">
            <div class="col-md-6 justify-content-center">
                <label class="form-label"> Adress </label>
                <input class="form-control" asp-for="fact.Project" />
                <input type="submit" value="Update Facts" class="btn btn-primary">
            </div>
        </div>


    </form>
</div>