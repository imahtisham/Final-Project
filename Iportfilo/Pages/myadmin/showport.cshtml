@page
@model Iportfilo.Pages.myadmin.showportModel
@{
}
<div class="container">
    <div class="row" justify-content-center>
        <div class="col-md-10">
            <table class="table">
                <thead>
                    <tr>
                        <th scope="col">#</th>
                        <th scope="col"> Title </th>
                        <th scope="col">Birthday</th>
                        <th scope="col">Website</th>
                        <th scope="col">Degree</th>
                        <th scope="col">Age</th>
                        <th scope="col">Phone No</th>
                        <th scope="col">Email</th>
                        <th scope="col">City</th>
                        <th scope="col"> Freelancer </th>
                        <th scope="col" colspan="2">Action</th>
                    </tr>
                </thead>
                <tbody>
                    @foreach (var item in Model.iportfiloes)
                    {
                        <tr>
                            <th scope="row">@item.Id</th>
                            <td>@item.Name</td>
                            <td>@item.Birthday</td>
                            <td>@item.Website</td>
                            <td>@item.Degree</td>
                            <td>@item.Age</td>
                            <td>@item.PhoneNo</td>
                            <td>@item.Email</td>
                            <td>@item.City</td>
                            <td>@item.Freelancer</td>
                            <td>
                                <a class="btn btn-danger" asp-page-handler="Delete" asp-route-Id="@item.Id"> Delete</a>
                            </td>
                            <td>
                                <a class="btn btn-primary" asp-page="editport" asp-route-Id="@item.Id"> Update</a>
                            </td>
                        </tr>
                    }

                </tbody>
            </table>
        </div>
    </div>

</div>